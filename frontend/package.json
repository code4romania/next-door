    "prettier": "^1.19.1",
    "redux-devtools-extension": "^2.13.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "jest": true
    },
    "plugins": [
      "prettier"
